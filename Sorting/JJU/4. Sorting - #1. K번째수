def solution(array, commands):
    # answer = []
    # for i in range(len(commands)):
    #     answer.append(sorted(array[commands[i][0]-1:commands[i][1]])[commands[i][2]-1])
    # return answer
    
    answer = []
    for x, y, z in commands:
        # x=2, y=5, z=3
        answer.append(sorted(array[x-1:y])[z-1])
        # i부터 j까지 중 k번째 원소
    return answer 
